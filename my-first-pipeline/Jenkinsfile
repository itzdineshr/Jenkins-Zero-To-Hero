pipeline {
  agent none
  stages {
    stage('Build') {  
      agent {
        docker { image 'maven:3.8.1-adoptopenjdk-11' }
      }
      steps {
        sh 'mvn --version'
        sh 'java --version'
        sh 'cd my-first-pipeline/'
        sh 'javac HelloWorld.java'
       
      }
    }
  }
}
